{
  "name": "@smartlyio/oats-runtime",
  "version": "2.8.0",
  "license": "MIT",
  "description": "Runtime for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/src/runtime.js",
  "types": "dist/src/runtime.d.ts",
  "scripts": {
    "test": "yarn jest",
    "oas": "yarn ts-node -r tsconfig-paths/register test/runtime-driver.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "yarn clean:dist && rm -f ./tmp/*.ts",
    "clean:dist": "rm -rf ./dist",
    "build": "yarn clean && yarn oas && yarn tsc && yarn lint && yarn test",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "lodash": "^4.17.20"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats": "2.8.0",
    "@smartlyio/oats-fast-check": "0.1.0",
    "@types/jest": "26.0.19",
    "@types/lodash": "4.14.167",
    "@types/node": "13.13.16",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "fast-check": "2.10.0",
    "jest": "26.6.3",
    "jsverify": "0.8.4",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "@smartlyio/oats-runtime": "<rootDir>/src/runtime"
    },
    "preset": "ts-jest",
    "testMatch": null
  }
}
